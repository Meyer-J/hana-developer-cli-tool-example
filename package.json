{
  "name": "hana-cli",
  "version": "3.202205.2",
  "description": "HANA Developer Command Line Interface",
  "main": "index.js",
  "bin": {
    "hana-cli": "./bin/cli.js"
  },
  "type": "module",
  "engines": {
    "node": "^14.0.0 || ^16.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "changelog": "node CHANGELOG",
    "types": "tsc  --declaration --allowJs --emitDeclarationOnly --outDir types --skipLibCheck",
    "postinstall": "npm link @sap/cds-dk --local"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SAP-samples/hana-developer-cli-tool-example.git"
  },
  "keywords": [
    "hana"
  ],
  "types": "types/index.d.ts",
  "dependencies": {
    "@graphql-tools/schema": "^8.3.11",
    "@sap/cds": "^5.9.5",
    "@sap/textbundle": "^4.1",
    "@sap/xsenv": "^3.2.2",
    "body-parser": "^1.20.0",
    "chalk": "^5.0.1",
    "check-node-version": "^4.2.1",
    "cli-highlight": "^2.1.11",
    "console.table": "~0.10",
    "debug": "^4.3.4",
    "dotenv": "^16.0.0",
    "easy-table": "1.2.0",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "glob": "^8.0.1",
    "graphql": "^15.8.0",
    "hdb": "0.19.2",
    "js-convert-case": "^4.2.0",
    "json-to-pretty-yaml": "^1.2.2",
    "json2csv": "^5.0.7",
    "latest-version": "^6.0",
    "marked": "^4.0.15",
    "marked-terminal": "^5.1.1",
    "node-xlsx": "^0.21",
    "node-zip": "^1.1.1",
    "odata2openapi": "^1.3",
    "open": "^8.2.1",
    "ora": "^6.1",
    "prompt": "^1.3.0",
    "properties-to-json": "^0.2.1",
    "sap-hdb-promisfied": "2.202205.1",
    "showdown": "^2.1.0",
    "swagger-ui-express": "^4.3",
    "terminaltools": "^2.1",
    "upath": "^2.0.1",
    "update-notifier": "^5.1.0",
    "uuid": "^8.3",
    "ws": "^8.6.0",
    "yargonaut": "1.1",
    "yargs": "^17.4.1"
  },
  "peerDependencies": {
    "@sap/cds-dk": ">=4"
  },
  "author": "SAP",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/SAP-samples/hana-developer-cli-tool-example/issues"
  },
  "homepage": "https://github.com/SAP-samples/hana-developer-cli-tool-example#readme",
  "devDependencies": {
    "@types/showdown": "^2.0.0",
    "@types/yargs": "^17.0.10",
    "eslint": "^8.15.0",
    "stringify-changelog": "^0.2.1"
  },
  "cds": {
    "preview": {
      "ui5": {
        "version": "1.101.1"
      }
    },
    "hana": {
      "journal": {
        "enable-drop": false,
        "change-mode": "alter"
      },
      "deploy-format": "hdbtable"
    },
    "requires": {
      "db": {
        "kind": "hana"
      }
    },
    "features": {
      "preserve_computed": true,
      "kibana_formatter": true,
      "audit_personal_data": true,
      "optimized_search": true,
      "rest_new_adapter": true,
      "graphql": true
    },
    "log": {
      "service": true
    }
  }
}
